<!-- index.ejs -->
<html>
	<head>
		<title>Contact Us</title>
		<link rel="stylesheet" href="/styles.css" />
	</head>
	<body>
		<%- include('navbar', {links: links}) %>
		<h1>You can reach us at:</h1>
		<ul>
			<% contact.forEach((c) => { %>
			<li><%= c.type %> : <%= c.text %></li>
			<% }) %>
		</ul>
		<%- include('footer') %>
	</body>
</html>
